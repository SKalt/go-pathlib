{
  "go.lintTool": "golangci-lint-v2",
  "go.lintFlags": [
    "--path-mode=abs",
  ],
  "go.formatTool": "custom",
  "go.alternateTools": {
    "customFormatter": "golangci-lint"
  },
  "go.formatFlags": [
    "fmt",
    "--stdin"
  ],
  "cSpell.words": [
    "devcontainers",
    "goimports",
    "golines",
    "itsmechlark",
    "libc",
    "pathlib",
    "stdlib"
  ],
}
